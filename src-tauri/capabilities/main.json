{
  "$schema": "./schemas/desktop-schema.json",
  "identifier": "run-app",
  "description": "permissions to run the app",
  "windows": [
    "main",
    "main-*"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "app:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "log:default",
    "window:default",
    "webview:default",
    "store:allow-get",
    "store:allow-set",
    "store:allow-save",
    "store:allow-has",
    "shell:allow-open"
  ]
}
